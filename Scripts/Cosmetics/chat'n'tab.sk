		set player's tablist name to "&a&r%fontimage ""betterranks:player""%<##E3E3E3>   %player%%{_d}%"
	if player has permission "donator":
		create name tag "z-donator"
		set suffix of nametag "z-donator" to " &f%fontimage ""betterranks:donator""%"
		add player to nametag "z-donator"
on PlayerAFKEvent:
	set {_p} to event.getPlayer()
	set {_d} to ""
	if {_p} has permission "donator":
		set {_d} to " %fontimage ""betterranks:donator""%"
	if event.isGoingAfk() = true:
		if {_p} has permission "owner":
			set {_p}'s tablist name to "&1&r%fontimage ""betterranks:owner""%<##FFFF7C>   %{_p}%%{_d}% &4&l*AFK*"
		else if {_p} has permission "admin":
			set {_p}'s tablist name to "&2&r%fontimage ""betterranks:admin""%<##FF7C7C>   %{_p}%%{_d}% &4&l*AFK*"
		else if {_p} has permission "mod":
			set {_p}'s tablist name to "&3&r%fontimage ""betterranks:mod""%<##FFD17C>   %{_p}%%{_d}% &4&l*AFK*"
		else if {_p} has permission "builder":
			set {_p}'s tablist name to "&4&r%fontimage ""betterranks:mvipplus""%<##70AAFF>   %{_p}%%{_d}% &4&l*AFK*"
		else if {_p} has permission "helper":
			set {_p}'s tablist name to "&5&r%fontimage ""betterranks:owner""%<##7EFF5D>   %{_p}%%{_d}% &4&l*AFK*"
		else if {_p} has permission "mvipplus":
			set {_p}'s tablist name to "&6&r%fontimage ""betterranks:mvipplus""%   %{_p}%%{_d}% &4&l*AFK*"
		else if {_p} has permission "mvip":
			set {_p}'s tablist name to "&7&r%fontimage ""betterranks:mvip""%   %{_p}%%{_d}% &4&l*AFK*"
		else if {_p} has permission "svip":
			set {_p}'s tablist name to "&8&r%fontimage ""betterranks:svip""%   %{_p}%%{_d}% &4&l*AFK*"
		else if {_p} has permission "vip":
			set {_p}'s tablist name to "&9&r%fontimage ""betterranks:vip""%   %{_p}%%{_d}%  &4&l*AFK*"
		else:
			set {_p}'s tablist name to "&a&r%fontimage ""betterranks:player""%<##E3E3E3>   %{_p}%%{_d}% &4&l*AFK*"
	else:
		if {_p} has permission "owner":
			set {_p}'s tablist name to "&1&r%fontimage ""betterranks:owner""%<##FFFF7C>   %{_p}%%{_d}%"
		else if {_p} has permission "admin":
			set {_p}'s tablist name to "&2&r%fontimage ""betterranks:admin""%<##FF7C7C>   %{_p}%%{_d}%"
		else if {_p} has permission "mod":
			set {_p}'s tablist name to "&3&r%fontimage ""betterranks:mod""%<##FFD17C>   %{_p}%%{_d}%"
		else if {_p} has permission "builder":
			set {_p}'s tablist name to "&4&r%fontimage ""betterranks:mvipplus""%<##70AAFF>   %{_p}%%{_d}%"
		else if {_p} has permission "helper":
			set {_p}'s tablist name to "&5&r%fontimage ""betterranks:owner""%<##7EFF5D>   %{_p}%%{_d}%"
		else if {_p} has permission "mvipplus":
			set {_p}'s tablist name to "&6&r%fontimage ""betterranks:mvipplus""%   %{_p}%%{_d}%"
		else if {_p} has permission "mvip":
			set {_p}'s tablist name to "&7&r%fontimage ""betterranks:mvip""%   %{_p}%%{_d}%"
		else if {_p} has permission "svip":
			set {_p}'s tablist name to "&8&r%fontimage ""betterranks:evip""%   %{_p}%%{_d}%"
		else if {_p} has permission "vip":
			set {_p}'s tablist name to "&8&r%fontimage ""betterranks:avip""%   %{_p}%%{_d}%"
		else:
			set {_p}'s tablist name to "&a&r%fontimage ""betterranks:player""%<##E3E3E3>   %{_p}%%{_d}%"