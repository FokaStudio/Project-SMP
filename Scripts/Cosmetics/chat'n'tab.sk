on join:
	set {_d} to ""
	if player has permission "donator":
		set {_d} to " %fontimage ""betterranks:donator""%"
	if player has permission "owner":
		create name tag "a-owner"
		set prefix of nametag "a-owner" to "%fontimage ""betterranks:owner""%<##FFFF7C> "
		add player to nametag "a-owner"
		set player's tablist name to "&1&r%fontimage ""betterranks:owner""%<##FFFF7C>   %player%%{_d}%"
	else if player has permission "admin":
		create name tag "b-admin"
		set prefix of nametag "b-admin" to "%fontimage ""betterranks:admin""%<##FF7C7C> "
