import:
	net.pl3x.purpur.event.PlayerAFKEvent

on chat:
	set {_d} to ""
	if player has permission "donator":
		set {_d} to " %fontimage ""betterranks:donator""%"
	if player has permission "owner":
		set the chat format to "%fontimage ""betterranks:owner""%<##FFFF7C> %player%%{_d}% &r&7» &r<##FFFF7C>%colored chat-message%"
	else if player has permission "admin":
		set the chat format to "%fontimage ""betterranks:admin""%<##FF7C7C> %player%%{_d}% &r&7» &r<##FF7C7C>%formatted chat-message%"
	else if player has permission "mod":
		set the chat format to "%fontimage ""betterranks:mod""%<##FFD17C> %player%%{_d}% &r&7» &r<##FFD17C>%colored chat-message%"
	else if player has permission "builder":
		set the chat format to "%fontimage ""betterranks:builder""%<##7C98FF> %player%%{_d}% &r&7» &r<##7C98FF>%colored chat-message%"
	else if player has permission "helper":
		set the chat format to "%fontimage ""betterranks:helper""%<##7EFF5D> &r%player%%{_d}% &r&7» &r<##7EFF5D>%colored chat-message%"
	else if player has permission "mvipplus":
		set the chat format to "%fontimage ""betterranks:mvipplus""% &r%player%%{_d}% &r&7» &r%colored chat-message%"
	else if player has permission "mvip":
		set the chat format to "%fontimage ""betterranks:mvip""% &r%player%%{_d}% &r&7» &r%colored chat-message%"
	else if player has permission "svip":
		set the chat format to "%fontimage ""betterranks:svip""% &r%player%%{_d}% &r&7» &r%chat-message%"
	else if player has permission "vip":
		set the chat format to "%fontimage ""betterranks:vip""% &r%player%%{_d}% &r&7» &r%chat-message%"
	else:
		set the chat format to "%fontimage ""betterranks:player""%<##E3E3E3> %player%%{_d}% &r&7» &r<##E3E3E3>%chat-message%"

